{
  "build": {
    "env": {
      "NODE_OPTIONS": "--max_old_space_size=4096",
      "JOBS": "1"
    }
  },
  "github": {
    "silent": true
  },
  "buildCommand": "pnpm install --no-frozen-lockfile && pnpm build",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "framework": "docusaurus",
  "outputDirectory": "build"
}
