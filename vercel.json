{
  "build": {
    "env": {
      "NODE_OPTIONS": "--max_old_space_size=8192",
      "JOBS": "1",
      "WEBPACK_CACHE": "false"
    }
  },
  "github": {
    "silent": true
  },
  "buildCommand": "pnpm install && pnpm build",
  "installCommand": "pnpm install",
  "framework": "docusaurus",
  "outputDirectory": "build"
}
